{
  "name": "real-time-quizz",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ],
  "scripts": {
    "install:all": "npm install",
    "prisma:generate": "npm run prisma:generate -w backend",
    "prisma:migrate": "npm run prisma:migrate -w backend",
    "dev": "concurrently -k -n FRONTEND,BACKEND \"npm run dev -w frontend\" \"npm run start:dev -w backend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
